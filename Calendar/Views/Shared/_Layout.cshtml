    <!-- INTERNAL Index js-->
    <script src="../../assets/js/index1-dark.js"></script>

    <!-- Custom js-->
    <script src="../../assets/js/custom.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
